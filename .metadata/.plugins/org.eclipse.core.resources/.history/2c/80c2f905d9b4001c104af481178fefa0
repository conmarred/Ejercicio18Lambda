package ejercicio;

//Defino interfaz funcionable
interface Operaciones{
	
	public Integer operacion(Integer i, Integer j);
}


public class Ejercicio19 {

	
	public static void main(String[] args) {
		Operaciones op = new Operaciones() {
			
			@Override
			public Integer operacion(Integer i, Integer j) {
				return i+j;
			}
		};

	}

}
